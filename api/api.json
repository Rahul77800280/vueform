{
  "elements": {
    "text": {
      "path": "src/components/elements/TextElement.js",
      "layout": "BaseElementLayout",
      "components": {
        
      },
      "mixins": ["BaseElement"],
      "features": ["form$", "theme", "input"],
      "props": {
        "schema": {
          "types": ["object"],
          "description": "Element schema",
          "default": "{}"
        }
      },
      "injects": {
        "form$": {
          "types": ["object"],
          "description": "Root Laraform component"
        }
      },
      "data": {
        "active": {
          "types": ["boolean"],
          "description": "Whether the element is active. Should not be used directly",
          "default": true
        }
      },
      "computed": {
        "data": {
          "types": ["object"],
          "description": "Element data"
        }
      },
      "methods": {
        "update": {
          "description": "Updates data",
          "return": "void",
          "params": {
            "value": {
              "description": "Value to be updated with",
              "types": ["string", "number"]
            },
            "triggerChange": {
              "description": "Whether change should be triggered",
              "types": ["boolean"]
            },
            "shouldValidate": {
              "description": "Whether element should be validated after the update",
              "types": ["boolean"]
            }
          }
        }
      },
      "events": {
        "change": {
          "description": "Triggered when the element's value is changed",
          "params": {
            "newValue": {
              "description": "The current value",
              "types": ["string", "number"]
            },
            "oldValue": {
              "description": "The previous value",
              "types": ["string", "number"]
            }
          }
        }
      },
      "slots": {
        "before": {
          "descriptions": "Before the element slot",
          "props": {
            "el$": {
              "description": "The element's component"
            }
          }
        }
      }
    }
  }
}