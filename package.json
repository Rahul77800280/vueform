{
    "name": "@laraform/laraform",
    "version": "1.1.17",
    "description": "Laraform Vue package",
    "homepage": "https://laraform.io",
    "bugs": {
        "url": "https://github.com/laraform/laraform/issues"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "authors": [
        {
            "name": "Adam Berecz",
            "email": "adam@laraform.io"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/laraform/laraform.git"
    },
    "main": "dist/full.js",
    "scripts": {
        "build:full": "cross-env BUNDLE=full node_modules/webpack/bin/webpack.js --progress --config build/webpack.build.config.js",
        "build:essentials": "cross-env BUNDLE=essentials node_modules/webpack/bin/webpack.js --progress --config build/webpack.build.config.js",
        "build:core": "cross-env BUNDLE=core node_modules/webpack/bin/webpack.js --progress --config build/webpack.build.config.js",
        "build": "npm run build:full; npm run build:essentials; npm run build:core",
        "test": "jest",
        "test-vue2": "VUE=2 jest --watchAll --verbose --config=jest.config.vue2.js",
        "test-vue3": "VUE=3 jest --watchAll --verbose --config=jest.config.vue3.js"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/runtime": "^7.3.1",
        "@testing-library/jest-dom": "^5.9.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/composition-api": "^1.0.0-beta.15",
        "babel-core": "^6.26.3",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.0",
        "flush-promises": "^1.0.2",
        "jest": "^26.0.1",
        "jest-environment-jsdom-sixteen": "^1.0.3",
        "jest-transform-stub": "^2.0.0",
        "json-loader": "^0.5.7",
        "mutationobserver-shim": "^0.3.7",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "typescript": "^4.0.3",
        "vue-jest-next": "npm:vue-jest@^5.0.0-alpha.4",
        "vue-jest-prev": "npm:vue-jest@^3.0.7",
        "vue-loader": "^15.5.1",
        "vue-next": "npm:vue@^3.0.0",
        "vue-prev": "npm:vue@^2.6.12",
        "vue-template-compiler": "^2.6.11",
        "vue-test-utils-next": "npm:@vue/test-utils@^2.0.0-beta.6",
        "vue-test-utils-prev": "npm:@vue/test-utils@^1.1.0",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.2.1",
        "webpack-notifier": "^1.7.0"
    },
    "dependencies": {
        "autosize": "^4.0.2",
        "axios": "^0.19.2",
        "bootstrap": "^4.4.1",
        "flatpickr": "^4.5.2",
        "locutus": "^2.0.12",
        "moment": "^2.27.0",
        "moxios": "^0.4.0",
        "places.js": "^1.14.0",
        "sortablejs": "^1.7.0",
        "sprintf-js": "^1.1.2",
        "trix": "^1.0.0",
        "vue-i18n": "^8.18.2",
        "vue-js-toggle-button": "^1.3.0",
        "vue-multiselect": "^2.1.3",
        "vue-reactive-provide": "^0.3.0",
        "vue-slider-component": "^2.8.2",
        "vue-text-mask": "^6.1.2",
        "vuex": "^3.4.0"
